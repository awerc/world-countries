{
  "extends": "eslint-config-airbnb",
  "env": {
    "browser": true,
    "es6": true
  },
  "rules": {
    "comma-dangle": 0,
    "linebreak-style": 0,
    "arrow-parens": ["error", "as-needed"],
    "consistent-return": [2, { "treatUndefinedAsUnspecified": true }],
    "jsx-a11y/no-static-element-interactions": 0,
    "jsx-a11y/no-noninteractive-element-interactions": 0,
    "jsx-a11y/tabindex-no-positive": 0,
    "max-len": [2, 200, 2, {
      "ignoreUrls": true,
      "ignoreComments": false
    }],
    "class-methods-use-this": 0,
    "react/require-default-props": 0,
    "react/no-array-index-key": 0,
    "react/forbid-prop-types": [2, { "forbid": ["any"] }]
  },
  "settings": {
    "import/resolver": {
      "alias": [
        ["Services", "../Scripts/Common/Services"],
        ["Utils", "../Scripts/Common/Utils"],
        ["Constants", "../Scripts/Common/Constants"],
        ["Containers", "../Scripts/Common/Containers"],
        ["Components", "../Scripts/Common/Components"],
        ["Decorators", "../Scripts/Common/Decorators"]
      ]
    }
  },
  "parser": "babel-eslint"
}
